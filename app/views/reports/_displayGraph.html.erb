ASKLHJLFK
<%= @formClass%>
<%= @form_id%>
<%= @section_id%>
<%= @Graphtype%>
<%if @Graphtype == "horizon"%>
	<%= bar_chart horizonbar_reports_path(:class => @class , :firstGroup => @form_id, :secondGroup => @section_id) , library: {
			exporting: {
            chartOptions: { 
                plotOptions: {
                    series: {
                        dataLabels: {
                            enabled: true
                        }
                    }
                }
            },
            scale: 3,
            fallbackToExportServer: false
        	},
	        chart: {
		        renderTo: 'previewgraph',
	            type: 'bar',
	            backgroundColor: @chartbackground,
	            borderColor: @bordercolor,
				borderRadius: @borderradius,
				borderWidth: @borderwidth,
				plotBackgroundImage: @plotimage
				
        	},
	        title: {text: @titledata, x: -20
	        },
	        subtitle: {text: @subtitledata , x:-20
	        },
	        yAxis: {
	            title: {
	                text: @ytitledata, x:-30
	            },
	            labels: {
                overflow: 'justify'
            	}
	        },
	        xAxis: {
	        	categories: [],
	            title: {
	                text: @xtitledata
	            }
	        },
	        tooltip: {
            valueSuffix: @valuesuffix,
            valuePrefix: @valueprefix
	        },
	        plotOptions: {
	            bar: {
	                dataLabels: {
	                    enabled: true
	                }
	            }
	        },
	        legend: {
	            layout: 'vertical',
	            align: 'right',
	            verticalAlign: 'top',
	            x: 0,
	            y: 80,
	            borderWidth: 1,
	            shadow: true
	        }
	    }
	%>
	
	
	
<%elsif @Graphtype == "vertical"%>
	<%= column_chart horizonbar_reports_path ,xtitle: @xtitledata, ytitle: @ytitledata, library: {
        exporting: {
            chartOptions: { 
                plotOptions: {
                    series: {
                        dataLabels: {
                            enabled: true
                        }
                    }
                }
            },
            scale: 3,
            fallbackToExportServer: false
        },
        
        chart: {
            type: 'column',
            	backgroundColor: @chartbackground,
	            borderColor: @bordercolor,
				borderRadius: @borderradius,
				borderWidth: @borderwidth,
				plotBackgroundImage: @plotimage
        	},
	        title: {text: @titledata, x: -20
	        },
	        subtitle: {text: @subtitledata , x:-20
	        },
	        tooltip: {
            valueSuffix: @valuesuffix,
            valuePrefix: @valueprefix
	        },
	        yAxis: {
	            title: {
	                text: @ytitledata, x:-30
	            }
	        },
	        xAxis: {
	            title: {
	                text: @xtitledata
	            },
	            categories: [
                
	            ],
	            crosshair: true
	        },
	        plotOptions: {
            column: {
                pointPadding: 0.2,
                borderWidth: 0
            	}
        	}
	    }
	%> 
<%elsif @Graphtype == "pie"%>		
	<%= pie_chart horizonbar_reports_path ,xtitle: @xtitledata, ytitle: @ytitledata, library: {
        exporting: {
            chartOptions: { 
                plotOptions: {
                    series: {
                        dataLabels: {
                            enabled: true
                        }
                    }
                }
            },
            scale: 3,
            fallbackToExportServer: false
        },
        tooltip: {
            valueSuffix: @valuesuffix,
            valuePrefix: @valueprefix
	        },
        pie_chart: {
	            type: 'column',
	            backgroundColor: @chartbackground,
	            borderColor: @bordercolor,
				borderRadius: @borderradius,
				borderWidth: @borderwidth,
				plotBackgroundImage: @plotimage
        	},
	        title: {text: @titledata, x: -20
	        },
	        subtitle: {text: @subtitledata , x:-20
	        },
	        yAxis: {
	            title: {
	                text: @ytitledata, x:-30
	            }
	        },
	        xAxis: {
	            title: {
	                text: @xtitledata
	            }
	        },
	        plotOptions: {
                pie: {
                    allowPointSelect: true,
                    cursor: 'pointer',
                    dataLabels: {
                        enabled: false
                    },
                    showInLegend: true
                }
            }
	    }
    %> 
<%end%>

