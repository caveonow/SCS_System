<p id="notice"><%= notice %></p>

<h1>Listing Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Staffname</th>
      <th>Staffage</th>
      <th>Staffic</th>
      <th>Staffemail</th>
      <th>Staffpassword</th>
      <th>Stafflogaccess</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @staffs.each do |staff| %>
      <tr>
        <td><%= staff.StaffName %></td>
        <td><%= staff.StaffAge %></td>
        <td><%= staff.StaffIC %></td>
        <td><%= staff.StaffEmail %></td>
        <td><%= staff.StaffPassword %></td>
        <td><%= staff.StaffLogAccess %></td>
        <td><%= link_to 'Show', staff %></td>
        <td><%= link_to 'Edit', edit_staff_path(staff) %></td>
        <td><%= link_to 'Destroy', staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff', new_staff_path %>
