<p id="notice"><%= notice %></p>

<h1>Listing Subquestions</h1>

<table>
  <thead>
    <tr>
      <th>Subquestiondesc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subquestions.each do |subquestion| %>
      <tr>
        <td><%= subquestion.SubquestionDesc %></td>
        <td><%= link_to 'Show', subquestion %></td>
        <td><%= link_to 'Edit', edit_subquestion_path(subquestion) %></td>
        <td><%= link_to 'Destroy', subquestion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subquestion', new_subquestion_path %>
