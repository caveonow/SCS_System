<h1>Testing Form</h1>

<table>
  <thead>
    <tr>
      <th>Formname</th>
      <th>Formdescription</th>
      <th>Formdatetime</th>
      <th>Formstatus</th>
      <th>FormId</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @forms.each do |testForm| %>
      <tr>
        <td><%= testForm.FormName %></td>
        <td><%= testForm.FormDescription %></td>
        <td><%= testForm.FormDateTime %></td>
        <td><%= testForm.FormStatus %></td>
        <td><%= testForm.id %></td>
        <td><%= link_to 'Select', form_select_path(:formId => testForm.id) %> <%= link_to 'TestNew', form_start_path(:formId => testForm.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>



