<%= form_for(@form) do |f| %>
  <% if @form.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@form.errors.count, "error") %> prohibited this form from being saved:</h2>

      <ul>
      <% @form.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :FormName %><br>
    <%= f.text_field :FormName %>
  </div>
  <div class="field">
    <%= f.label :FormDescription %><br>
    <%= f.text_area :FormDescription %>
  </div>
  <div class="field">
    <%= f.label :FormDateTime %><br>
    <%= f.datetime_select :FormDateTime %>
  </div>
  <div class="field">
    <%= f.label :FormStatus %><br>
    <%= f.text_field :FormStatus %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
