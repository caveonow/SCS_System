<table>
  <thead>
    <tr>
      <th>Survey Name</th>
      <th>Description</th>
      <th>Formstatus</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @user_form_inProgress.each do |form| %>
      <tr>
        <td><%= form.FormName %></td>
        <td><%= form.FormDescription %></td>
        <td>In Progress</td>
        <td><%= link_to 'Continue Survey', user_selected_survey_forms_path(:form_id => form.id, :user_id => current_user.id) %>  </td>
      </tr>
    <% end %>
  </tbody>
</table>
